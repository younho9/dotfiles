[init]
	defaultBranch = main

[user]
	name = Younho Choo
	email = younho9.choo@gmail.com

[alias]
  # See https://docs.gitlab.com/ee/user/project/merge_requests/reviews/#checkout-locally-by-adding-a-git-alias
  mr = !sh -c 'git fetch $1 merge-requests/$2/head:$1/merge-requests/$2 && git checkout $1/merge-requests/$2' -
  pr = !sh -c 'git fetch $1 pull/$2/head:$1/pull-requests/$2 && git checkout $1/pull-requests/$2' -
	graph = log --branches --graph --oneline
	new = checkout -b

[core]
	excludesfile = /Users/younho9/.gitignore_global
	ignorecase = false

[pull]
	rebase = true

[commit]
	template = /Users/younho9/.gitmessage.txt

[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true

[includeIf "gitdir:~/repositories/company/"]
	path = ~/repositories/company/.company.inc
